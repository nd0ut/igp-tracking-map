    "@remix-run/react": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "@remix-run/server-runtime": "^1.4.3",
    "@types/mapbox__mapbox-gl-draw": "^1.2.3",
    "daisyui": "^2.14.3",
    "mapbox-gl": "^2.8.2",
    "npm-run-all": "^4.1.5",
